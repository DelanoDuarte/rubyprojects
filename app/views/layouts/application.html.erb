<!DOCTYPE html>
<html>
<head>
  <title>Ruby</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div align="top">

  <% if user_signed_in? %>
  <ul>
    <li><%= link_to "SigOut", destroy_user_session_path, method: :delete %></li>
  </ul>
  <% else %>
  <ul>
    <li><%= link_to "SigIn", new_user_registration_path %></li>
</ul>
  <% end %>

</div>


<%= yield %>

</body>
</html>
